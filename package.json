{
  "name": "team-maker-js",
  "version": "1.4.5",
  "description": "A program to easily create 5v5 teams for League of Legends.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "keywords": [],
  "author": {
    "name": "Aznoca",
    "email": "aznoca324@naver.com"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "productName": "Team_Maker",
    "appId": "com.example.teammaker",
    "artifactName": "TeamMaker-Setup-${version}.${ext}",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "Acronica",
      "repo": "Team_Maker",
      "releaseType": "release"
    }
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bindings": "^1.5.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.5",
    "electron-updater": "^6.1.1",
    "lcu-connector": "^2.1.4",
    "node-gyp": "^10.1.0"
  }
}